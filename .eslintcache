iewed</h2>\r\n        {recentlyViewed.slice(1, 5).map((product) => (\r\n          <div key={product.id} className=\"thubmnail-recent\">\r\n            <img\r\n              src={\r\n                imageDirectory(product.name) !== \"\"\r\n                  ? require(`../../assets/img/produts-img/${imageDirectory(\r\n                      product.name\r\n                    )}/${product.imageName}`).default\r\n                  : require(`../../assets/img/noImage.jpg`).default\r\n              }\r\n              className=\"recent-thumb\"\r\n              alt=\"\"\r\n            />\r\n            <h2>\r\n              <Link\r\n                to={`/${imageDirectory(product.name)}/${\r\n                  product.id\r\n                }/${product.name.replaceAll(\" \", \"_\")}.html`}\r\n              >\r\n                {product.name}\r\n              </Link>\r\n            </h2>\r\n            <Price\r\n              stylin\r\n              discount={product.discountRate}\r\n              price={product.price}\r\n              stylingClass=\"product-sidebar-price\"\r\n            />\r\n          </div>\r\n        ))}\r\n      </div>\r\n      <OtherBrands otherBrands={otherBrands} />\r\n    </>\r\n  );\r\n};\r\n\r\nexport default SideBar;\r\n","C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\format\\price.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\singlePromo.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\branding\\otherBrands.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\cart\\emptyCart.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\products\\TopProducts.js",["295"],"import ProductSummary from \"./ProductSummary\";\r\n\r\nconst TopProducts = (props) => {\r\n  return (\r\n    <div className=\"col-md-4\">\r\n      <div className=\"single-product-widget\">\r\n        <h2 className=\"product-wid-title\">{props.title}</h2>\r\n        <a href=\"\" className=\"wid-view-more\">\r\n          View All\r\n        </a>\r\n        {props.products.map((product) => (\r\n          <ProductSummary key={product.id} product={product} />\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default TopProducts;\r\n","C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\products\\ProductSummary.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\errorBloc.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\routes\\Routes.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\products\\CrossSells.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\products\\ProductWidgets.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\pages\\HomePage.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\pages\\CartPage.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\pages\\NotFoundPage.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\Spinner.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\Banner.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\branding\\OtherBrands.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\format\\Price.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\Breadcrumb.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\cart\\CartContent.js",["296"],"import { Link, withRouter } from \"react-router-dom\";\r\nimport EmptyCart from \"./EmptyCart\";\r\nimport Spinner from \"../layout/Spinner\";\r\nconst cartContent = (props) => {\r\n  const { cart, cartLoading } = props;\r\n\r\n  const imageDirectory = (imageName) => {\r\n    if (imageName.startsWith(\"apple\")) return \"Apple\";\r\n    if (imageName.startsWith(\"sony\")) return \"Sony\";\r\n    if (imageName.startsWith(\"lg\")) return \"LG\";\r\n    if (imageName.startsWith(\"samsung\")) return \"Samsung\";\r\n    if (imageName.startsWith(\"huawei\")) return \"Huawei\";\r\n    return \"\";\r\n  };\r\n\r\n  const checkoutPageHandler = () => {\r\n    props.history.push(\"/checkout.html\");\r\n  };\r\n\r\n  const changeQuantityForCart = (event, cartItem) => {\r\n    const value = parseInt(event.target.value);\r\n    props.changeQuantityHandler(cartItem, value);\r\n  };\r\n\r\n  const changeQuantityByOne = (cartItem, operation) => {\r\n    const index = props.cart.items.findIndex((item) => item.id === cartItem);\r\n    let quantity = props.cart.items[index].qty;\r\n    if (operation === \"add\") {\r\n      quantity = parseInt(quantity) + 1;\r\n      props.changeQuantityHandler(cartItem, quantity);\r\n    } else if (operation === \"remove\") {\r\n      quantity = quantity - 1;\r\n      props.changeQuantityHandler(cartItem, quantity);\r\n    } else if (operation === \"deleteEntry\") {\r\n      props.changeQuantityHandler(cartItem, 0);\r\n    }\r\n  };\r\n\r\n  return cartLoading ? (\r\n    <Spinner />\r\n  ) : cart.items.length !== 0 ? (\r\n    <table cellSpacing=\"0\" className=\"shop_table cart\">\r\n      <thead>\r\n        <tr>\r\n          <th className=\"product-remove\">&nbsp;</th>\r\n          <th className=\"product-thumbnail\">&nbsp;</th>\r\n          <th className=\"product-name\">Product</th>\r\n          <th className=\"product-price\">Price</th>\r\n          <th className=\"product-quantity\">Quantity</th>\r\n          <th className=\"product-subtotal\">Total</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        {cart.items.map((cartItem) => (\r\n          <tr key={cartItem.id} className=\"cart_item\">\r\n            <td className=\"product-remove\">\r\n              <a\r\n                title=\"Remove this item\"\r\n                className=\"remove\"\r\n                href=\"#\"\r\n                onClick={() => changeQuantityByOne(cartItem.id, \"deleteEntry\")}\r\n              >\r\n                <span aria-hidden=\"true\">&times;</span>\r\n              </a>\r\n            </td>\r\n\r\n            <td className=\"product-thumbnail\">\r\n              <Link\r\n                to={`/${imageDirectory(cartItem.imageName)}/${\r\n                  cartItem.id\r\n                }/${cartItem.name.replaceAll(\" \", \"_\")}.html`}\r\n              >\r\n                <img\r\n                  width=\"145\"\r\n                  height=\"145\"\r\n                  alt=\"poster_1_up\"\r\n                  className=\"shop_thumbnail\"\r\n                  src={\r\n                    require(`../../assets/img/produts-img/${imageDirectory(\r\n                      cartItem.imageName\r\n                    )}/${cartItem.imageName}`).default\r\n                  }\r\n                />\r\n              </Link>\r\n            </td>\r\n\r\n            <td className=\"product-name\">\r\n              <Link\r\n                to={`/${imageDirectory(cartItem.imageName)}/${\r\n                  cartItem.id\r\n                }/${cartItem.name.replaceAll(\" \", \"_\")}.html`}\r\n              >\r\n                {cartItem.name}\r\n              </Link>\r\n            </td>\r\n\r\n            <td className=\"product-price\">\r\n              <span className=\"amount\">{cartItem.price}€</span>\r\n            </td>\r\n\r\n            <td className=\"product-quantity\">\r\n              <div className=\"quantity buttons_added\">\r\n                <input\r\n                  type=\"button\"\r\n                  className=\"minus\"\r\n                  value=\"-\"\r\n                  disabled={cartItem.qty <= 1}\r\n                  onClick={() => changeQuantityByOne(cartItem.id, \"remove\")}\r\n                />\r\n                <input\r\n                  type=\"number\"\r\n                  size=\"4\"\r\n                  className=\"input-text qty text\"\r\n                  title=\"Qty\"\r\n                  value={cartItem.qty}\r\n                  onChange={(event) =>\r\n                    changeQuantityForCart(event, cartItem.id)\r\n                  }\r\n                  onKeyDown={(event) => {\r\n                    event.preventDefault();\r\n                  }}\r\n                  min=\"1\"\r\n                  step=\"1\"\r\n                />\r\n                <input\r\n                  type=\"button\"\r\n                  className=\"plus\"\r\n                  value=\"+\"\r\n                  onClick={() => changeQuantityByOne(cartItem.id, \"add\")}\r\n                />\r\n              </div>\r\n            </td>\r\n\r\n            <td className=\"product-subtotal\">\r\n              <span className=\"amount\">{cartItem.price * cartItem.qty} €</span>\r\n            </td>\r\n          </tr>\r\n        ))}\r\n\r\n        <tr>\r\n          <td className=\"actions\" colSpan=\"6\">\r\n            <input\r\n              type=\"button\"\r\n              onClick={() => {\r\n                checkoutPageHandler();\r\n              }}\r\n              value=\"Checkout\"\r\n              name=\"proceed\"\r\n              className=\"checkout-button button alt wc-forward\"\r\n            />\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  ) : (\r\n    <EmptyCart />\r\n  );\r\n};\r\n\r\nexport default withRouter(cartContent);\r\n","C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\cart\\CartTotal.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\BrandsArea.js",[],"C:\\Users\\Alaeddine-OMRI\\Documents\\Formation React\\formation\\src\\components\\layout\\Car